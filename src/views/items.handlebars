<div class="navbar">
    <div class="user-info">
        <p>{{user.email}}</p>
        <a href="/api/sessions/logout"><button>logout</button></a>
    </div>
</div>

<h1>items</h1>

<div class="items-container">
    {{#each items}}
        <div class="item-card">
            <a href="/items/{{this._id}}"><h3>{{this.description}}</h3></a>
            <small>{{this._id}}</small>
            <p>category: {{this.category}}</p>
            <p>price: {{this.price}}</p>
            <button>Add to cart</button>
        </div>
    {{/each}}
</div>


<div >
    {{#if hasPrevPage}}
        <a href="{{prevLink}}"><button >Prev</button></a>
    {{else}}
        <button disabled>Prev</button>
    {{/if}}

    {{#if hasNextPage}}
   
        <a href="{{nextLink}}"><button >Next</button></a>
    </button>
    {{else}}
        <button class='btnPageDisabled' disabled>Next</button>
    {{/if}}
</div>